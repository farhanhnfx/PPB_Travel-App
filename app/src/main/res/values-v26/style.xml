<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="rounded_stroke">
        <item name="strokeColor">@color/myrtie_green</item>
        <item name="strokeWidth">3dp</item>
        <item name="cornerRadius">15dp</item>
        <item name="backgroundTint">@color/white</item>
        <item name="android:textColor">@color/dark</item>
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="marginHorizontal">4dp</item>
    </style>
    <style name="rounded_stroke_filled">
        <item name="strokeColor">@color/myrtie_green</item>
        <item name="strokeWidth">2dp</item>
        <item name="cornerRadius">10dp</item>
        <item name="backgroundTint">@color/ash_gray</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="marginHorizontal">4dp</item>
        <item name="android:padding">10dp</item>
    </style>
</resources>